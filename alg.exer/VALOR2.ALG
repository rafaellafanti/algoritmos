  algoritmo "valor2"

 var
  num1, num2: inteiro

 inicio

  escreva("Digite o primeiro valor: ")
  leia(num1)
  escreva("Digite o segundo valor: ")
  leia(num2)

    se(num1 > num2)entao
     escreva(num1, " é maior")
    senao
     escreva(num2, " é maior")
    fimse

 fimalgoritmo

 