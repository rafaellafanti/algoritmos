  algoritmo "cnh"
  
  var
    nome, idade : inteiro
    
  inicio
  
   escreva("Digite seu nome: ")
   leia(nome)
   escreva("Digite sua idade: ")
   leia(idade)
   
    se(idade >= 18)entao
     escreva("Voce ja pode tirar a cnh!")
    senao(idade < 18)entao
     escreva("Voce ainda não pode tirar a cnh")
   fimse
   
  fimalgoritmo
  