   algoritmo "media"
   
   var
   a, b: inteiro
   m: real

   inicio
   escreva("Informe dois numeros inteiros: ")
   leia(a,b)
   m <- (a+b)/2
   escreval("A media entre ",a, " e ",b, " é " ,m)
   
   fimalgoritmo