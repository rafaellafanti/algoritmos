   algoritmo "formulaD"
   
   var
   a, b, c, d: real

   inicio
   escreva("Informe tres numeros: ")
   leia(a,b,c)
   d <- (a+c-b)
   escreva("A soma de ", a, " mais " ,c, " menos ",b, " é ", d)
   
   fimalgoritmo