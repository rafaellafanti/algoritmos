   algoritmo "formulaZ"
   
   var
   l, a, c, z: real

   inicio
   escreva("Informe tres numeros: ")
   leia(l,a,c)
   z <- (l+a*2)/c
   escreva("A multiplicaçao de ",a," vezez 2, mais ",l," dividido por ", c, "é" ,z)
   
   fimalgoritmo